# Session tracking
.session-decisions
.claude/

# Large runtime directories
projects/
todos/
debug/
plugins/
cache/
file-history/
shell-snapshots/
paste-cache/
session-env/
statsig/
telemetry/
ide/
tasks/
chrome/
tmp/
fudge-traces/
image-cache/

# External tool data
.gemini-clipboard/

# Session history
history.jsonl
stats-cache.json

# Plan files (session-specific)
plans/

# Archive (recoverable deletions, not tracked)
.archive/

